local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Key system | dsc.gg/v3rge | v3rg.", HidePremium = false, SaveConfig = save, ConfigFolder = "OrionTest", IntroEnabled = true, IntroText = "V3rge's bypassing society"})

function CorrectKeyNotification()
OrionLib:MakeNotification({
    Name = "To get the script you must complete the key system",
    Content = "The Key is at: https://dsc.gg/v3rge",
    Image = "rbxassetid://4483345998",
    Time = 10
})
end

_G.Key = "&6IvM84n0TjaY!"
_G.KeyInput = "string"

function MakeScriptHub()
loadstring(game:HttpGet("https://raw.githubusercontent.com/notkarlsalt/---/1378aeb74c25ab2e9ce191788c2efd5ea14efdbe/vahicg%20orion"))()
    end

function CorrectKeyNotification()
OrionLib:MakeNotification({
    Name = "Correct Key!",
    Content = "You have entered the correct key!",
    Image = "rbxassetid://4483345998",
    Time = 5
})
end

function IncorrectKeyNotification()
OrionLib:MakeNotification({
    Name = "Incorrect Key!",
    Content = "You have entered the incorrect key!",
    Image = "rbxassetid://4483345998",
    Time = 5
})
end

local Tab = Window:MakeTab({
    Name = "Home",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local Section = Tab:AddSection({
    Name = "Key System"
})

Tab:AddTextbox({
    Name = "Enter key",
    Default = "",
    TextDisappear = true,
    Callback = function(Value)
        _G.KeyInput = Value
    end	  
})

Tab:AddButton({
    Name = "Check Key",
    Callback = function()
    if _G.KeyInput == _G.Key then
              MakeScriptHub()
                CorrectKeyNotification()
            else
         IncorrectKeyNotification()
        end    
   end
})

Tab:AddParagraph("The key is in our Discord: https://dsc.gg/v3rge.","Verify first, then head to the key channel and copy-paste the key! Remember to remove any trailing spaces if you're on mobile.")
    
Tab:AddButton({
    Name = "Discord | Click To Copy",
    Callback = function()
game.StarterGui:SetCore("SendNotification", {Title="Some executors dont support this feature"; Text="If your executor does, then it was copied to your clipboard"; Duration=5;})
                wait(0.5)
setclipboard("https://dsc.gg/v3rge")
    end
}) 

  Tab:AddButton({
    Name = "Close Interface",
    Callback = function()
OrionLib:Destroy()
    end
})
